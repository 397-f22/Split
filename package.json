{
  "name": "split",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.10.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "firebase": "^9.12.1",
    "jsdom": "^20.0.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.4.2",
    "uuid": "^9.0.0",
    "vitest": "^0.25.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "cy:open": "cypress open --e2e",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "set REACT_APP_EMULATE=true&& firebase emulators:exec --import=./saved-data \"npm start\"",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'",
    "em:run": "start-server-and-test em:exec http://127.0.0.1:3000 cy:open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.25.3",
    "cypress": "^11.1.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.2.4"
  }
}
